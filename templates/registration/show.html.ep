% layout 'mealmaps', title => $user->{name};
% content headline => '';
% content main_form => '';

% content overlay => begin
  <h2>Build a Retirement Plan Proposal today!</h2>
  <p>Welcome, <%= user->name %> of <%= user->tpa->name %></p>
  <p>Fill in the form below to prepare a new proposal today!</p>
% end
% content solid => begin
  <h2><%= $user->{user_id} %></h2>
  <p><%= $user->{name} %></p>
  %= link_to 'Edit' => edit_profile => {user_id => $user->{user_id}}
% end
